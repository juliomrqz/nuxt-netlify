  "homepage": "https://marquez.co/docs/nuxt-netlify/?utm_source=npm&utm_medium=readme&utm_campaign=nuxt-netlify",
  "dependencies": {
    "consola": "^2.15.3",
    "fs-extra": "^9.1.0",
    "lodash.template": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
